{
  "name": "anomalyce/atomic",
  "description": "A Laravel and Inertia.js adapter layer for Atomic.",
  "type": "library",
  "license": "MIT",
  "authors": [
    { "name": "Anomalyce" }
  ],
  "require": {
    "php": ">=8.1",
    "fruitcake/laravel-cors": "~2.2",
    "laravel/framework": "~9.8",
    "laravel/tinker": "~2.7",
    "tightenco/ziggy": "~1.4",
    "inertiajs/inertia-laravel": "~0.5"
  },
  "require-dev": {
    "php": ">=8.1.1",
    "facade/ignition": "~2.17",
    "nunomaduro/collision": "~6.2",
    "fakerphp/faker": "~1.19",
    "mockery/mockery": "~1.5",
    "pestphp/pest": "~1.21",
    "pestphp/pest-plugin-laravel": "~1.2"
  },
  "minimum-stability": "dev",
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Anomalyce\\Atomic\\Adapter\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Anomalyce\\Atomic\\Adapter\\AdapterServiceProvider"
      ]
    }
  }
}
